<% @vets.each do |vet| %>
  <table>
    <tr>
      <td><a href = "/vets/<%= vet.id%>" ><%= vet.name%> </a></td>
    </tr>
  </table>
<% end %>

<!-- Add tabindex="x" to form Objects -->
<h3>Add a vet</h3>
<div class="form-wrapper">
  <form method="post" action="/vets">
  <label for="vet-name-field">Vet Name:</label>
  <input required
  type="text"
  name="name"
  id="vet-name-field"
  />
  <input type="submit" value="Add a Vet">
</form>
</div>
