<!-- Add tabindex="x" to form Objects -->
<h2>Edit <%= @vet.name %></h2>

<div class="form-wrapper">
  <form method="post" action="/vets/<%= @vet.id %>">

    <div class="input_wrapper">
      <label for="vet-name-field">Vet Name:</label>
      <input
      type="text"
      name="name"
      id="vet-name-field"
      value="<%= @vet.name %>"
      />
    </div>

    <div class="input_wrapper">
      <label for="vet-mobile-field">Vet Mobile:</label>
      <input
      type="text"
      name="name"
      id="vet-mobile-field"
      value="<%= @vet.mobile %>"
      />
    </div>

    <div class="input_wrapper">
      <input type="submit" value="Update Vet">
    </div>

  </form>
</div>
